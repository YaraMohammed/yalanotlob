<nav>
	<a href="/">Home</a>
	<a href="/friends">Friends</a>
	<a href="/groups">Groups</a>
	<a href="/orders">Orders</a>
	<a href="/profile">{{user.name}}</a>
	<a href="/" onclick="document.cookie = 'token=;expires=Thu, 01 Jan 1970 00:00:00 GMT'">Logout</a>
	<div>
		<div>
			Notifications
		</div>
		<ul id='notifications'>
			{{#each orderRequests}}
			<li>
				<a href="/user/{{owner}}">{{name}}</a> has invited you to his order.
				{{#ifEq requests "waiting"}}
				{{#ifEq status "waiting"}}
				<a href="/order/{{_id}}/accept">Join</a>
				{{else}}
				<a href="/order/{{_id}}">Finished</a>
				{{/ifEq}}
				{{else}}
				<a href="/order/{{_id}}">Joined</a>
				{{/ifEq}}
			</li>
			{{/each}}
		</ul>
	    <div>
	        <a href="/notifications">View all notifications</a>
	    </div>
	</div>
</nav>
