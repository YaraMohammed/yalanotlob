<!-- User Information Container  -->
<BR>
<div class="container">
<div class="row">
	<!-- Profile -->
	<div class="col-md-12">
		<div class="panel panel-primary-custom">
		  <div class="panel-heading"><h3 class="panel-title">Profile Page</h3></div>
			<div class="panel-body fixed-panel">
					<img src="/uploads/{{user.imageUrl}}" class="responsive"><BR>
					<h3>
						<span class="glyphicon glyphicon-user"></span>
						<p id="userName" style="display: inline">{{user.name}}<p>
					</h3>
					<h3>
						<span class="glyphicon glyphicon-envelope"></span>
						<p id="userEmail" style="display: inline">{{user._id}}</p>
					</h3>
			</div>
		</div>
		<!-- // Profile -->
	</div>
</div>
<!-- // User Information Container -->
